                                <p style="margin: 0px 0px -10px; width:80px;height: 20px;overflow: hidden;">{{app.getUser().getUserNicename()}}</p>
                                <img src="{{ asset('images/border-line.png') }}" style="width:80px">
                                <p style="margin: -8px 0px 0px;">Joined
                                    {{app.getUser().getCreatedAt()|date("d/m/Y")}}</p>
