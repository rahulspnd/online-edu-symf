                                <a class="btn menu_user_button" href="{{ path('user_profile', { 'encoded_user_id': app.getUser().getEncodedUserId() }) }}">
                                    <i class="fa fa-street-view fa-lg"></i>&nbsp;My Profile</a>
                                <a class="btn menu_user_button" href="{{ path('user_profile', { 'encoded_user_id': app.getUser().getEncodedUserId() }) }}?tab=privacy">
                                    <i class="fa fa-lock fa-lg"></i>&nbsp;Privacy settings</a>
                                <a class="btn menu_user_button" href="/app/logout">
                                    <i class="fa fa-sign-out fa-lg"></i>&nbsp;Logout</a>
                            </div>
                        </ul>
                    </li>

                    {% else %}
                        <style type="text/css">
                            ._qs_login_btn_new{
                                background-color: transparent;color: #fff;position: relative;top: 10px;padding: 8px 24px 8px 24px;border-radius: 3px;
                            }

                            @media (max-width: 768px) {
                                ._qs_login_btn_new{
                                    background-color: transparent;color: #fff;position: relative;top: -2px;padding: 8px 24px 8px 24px;border-radius: 3px;
                                }
                            }
                        </style>
                        <li class="dropdown xs_sm_visible second_half" style="">
                          <button onclick="_get_loginform();" class="_qs_login_btn_new" style="font-size: 12px;border: 1px solid #fff;">Login</button>
