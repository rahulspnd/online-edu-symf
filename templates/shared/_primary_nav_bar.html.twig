                        {% if app.request.attributes.get('_route') != "home_page" and app.request.attributes.get('_route') != "webinar_landing_page_new" and app.request.attributes.get('_route') != "admission_v121" %}

                             <li class="dropdown xs_sm_visible second_half" style="">
                                <button id="{{app.request.attributes.get('_route')}}" class="btn center-block join_now_btn_hp" onclick="_get_registerform();" style='width: 125px;font-size: 12px;margin-top: 10px;padding: 7px 5px 7px 5px;box-shadow: 0 3px 0 0 #2ea920;border-radius: 7px;margin-left: 10px;background: #36bb1c;font-weight: 100;'>Join For Free </button>
                            </li>
                        {% endif %}
                    {% endif %}
                </ul>
            </div>
